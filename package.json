    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.26",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "recharts": "^3.2.1",
    "tailwindcss": "^4.1.13",
    "zod": "^4.1.13",
    "@aws-sdk/client-cloudwatch-logs": "^3.922.0",
    "@aws-sdk/client-codebuild": "^3.922.0",
    "@aws-sdk/client-codepipeline": "^3.922.0",
    "@aws-sdk/client-codestar-connections": "^3.922.0",
    "@aws-sdk/client-cost-explorer": "^3.908.0",
    "@aws-sdk/client-ec2": "^3.896.0",
    "@aws-sdk/client-rds": "^3.883.0",
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/client-sts": "^3.883.0",
